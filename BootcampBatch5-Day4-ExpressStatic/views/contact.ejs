<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Contact</h1>
    <table border="2">
        <tr>
            <th>Nama</th>
            <th>Nomor Telp</th>
        </tr>
        <% if (contact.length) { %>
        <% contact.forEach((item) => { %>
        <tr>
            <td><%= item.nama %></td>
            <td><%= item.notelp %></td>
        </tr>
        <% }) %>
        <% } else { %>
        <tr>
            <td colspan="2">Tidak ada data</td>
        </tr>
        <% } %>
    </table>
</body>
</html>